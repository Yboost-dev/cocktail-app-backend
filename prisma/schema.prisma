generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  firstname String
  lastname  String
  role      String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
